{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "instapaper",
  "title": "Instapaper",
  "description": "Adds stuff into instapaper",
  "icon": "command-icon.png",
  "author": "davesnx",
  "categories": [
    "Applications"
  ],
  "license": "MIT",
  "commands": [
    {
      "name": "index",
      "title": "Run Script",
      "description": "Template for running a script without any view",
      "mode": "no-view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.69.0",
    "crypto": "^1.0.1",
    "node-fetch": "^3.3.2",
    "oauth-1.0a": "^2.2.6"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.6",
    "@types/node": "20.8.10",
    "@types/react": "18.2.27",
    "eslint": "^8.51.0",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "npx @raycast/api@latest publish"
  }
}
